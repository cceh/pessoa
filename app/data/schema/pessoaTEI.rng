         <ref name="tei_att.global.attribute.xmlid"/>
         <ref name="tei_att.global.linking.attribute.corresp"/>
         <ref name="tei_att.global.source.attribute.source"/>
         <ref name="tei_att.docStatus.attributes"/>
         <optional>
            <attribute name="type">
