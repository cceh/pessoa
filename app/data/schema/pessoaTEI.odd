            <titlePage>
                <docTitle>
                    <titlePart type="main">TEI Customization for the Digital Edition of Fernando
                        Pessoa</titlePart>
                    <titlePart type="sub">generated by Roma 4.10</titlePart>
                </docTitle>
                <docAuthor>written by Ulrike Henny-Krahmer and Erik Renz</docAuthor>
                <docDate>2017–2023</docDate>
